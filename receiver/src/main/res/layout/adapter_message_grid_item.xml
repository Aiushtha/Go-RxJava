<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:orientation="horizontal"
        android:layout_gravity="center"
        android:gravity="center"
        >
        <TextView
            android:singleLine="true"
            android:textColor="@android:color/white"
            android:textSize="14dp"
            android:id="@+id/laber_subject"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="subject:"/>
        <TextView
            android:singleLine="true"
            android:id="@+id/tv_subject"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=""
            android:textColor="@android:color/white"
            android:textSize="14dp"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:orientation="horizontal"
        android:layout_gravity="center"
        android:gravity="center"
        >
        <TextView
            android:singleLine="true"
            android:textColor="@android:color/white"
            android:textSize="14dp"
            android:id="@+id/laber_id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="id:"/>
        <TextView
            android:singleLine="true"
            android:id="@+id/tv_id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=""
            android:textColor="@android:color/white"
            android:textSize="14dp"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:orientation="horizontal"
        android:layout_gravity="center"
        android:gravity="center"
        >
        <TextView
            android:textColor="@android:color/white"
            android:textSize="14dp"
            android:id="@+id/laber_tag"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="tag:"
            android:singleLine="true"/>
        <TextView
            android:id="@+id/tv_tag"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=""
            android:textColor="@android:color/white"
            android:textSize="14dp"
            android:singleLine="true"/>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:orientation="horizontal"
        android:layout_gravity="center"
        android:gravity="center"
        >
        <TextView
            android:textColor="@android:color/white"
            android:textSize="14dp"
            android:id="@+id/laber_type"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="type:"
            android:singleLine="true"/>
        <TextView
            android:id="@+id/tv_type"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=""
            android:textColor="@android:color/white"
            android:textSize="14dp"
            android:singleLine="true"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:orientation="horizontal"
        android:layout_gravity="center"
        android:gravity="center"
        >
        <TextView
            android:textColor="@android:color/white"
            android:textSize="14dp"
            android:id="@+id/laber_src"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="src:"
            android:singleLine="true"/>
        <TextView
            android:id="@+id/tv_src"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=""
            android:textColor="@android:color/white"
            android:textSize="14dp"
            android:singleLine="true"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:orientation="horizontal"
        android:layout_gravity="center"
        android:gravity="center"
        >
        <TextView
            android:textColor="@android:color/white"
            android:textSize="14dp"
            android:id="@+id/laber_level"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="level:"
            android:singleLine="true"/>
        <TextView
            android:id="@+id/tv_level"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=""
            android:textColor="@android:color/white"
            android:textSize="14dp"
            android:singleLine="true"/>
    </LinearLayout>
</LinearLayout>